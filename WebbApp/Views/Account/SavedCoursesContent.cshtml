@model List<Infrastructure.Entities.SavedCourseEntity>
    <h1>Saved Courses</h1>

<div class="profile-info">
    <partial name="~/Views/Account/_AccountProfile.cshtml" />
</div>


    <table>
        <tr>
            <th>Course ID</th>
            <th>Title</th>
            <th>Author</th>
            <!-- Add more columns as needed -->
        </tr>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.CourseId</td>
                <td>@course.Title</td>
                <td>@course.Author</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </table>
